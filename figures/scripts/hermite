#run this script from figures directory!
set title 'Hermite Interpolation'
set xlabel 'x'
set ylabel 'f(x)'
set grid
set term png
set datafile separator ","
set output './output/hermite.png'
# add fixed points from initial task
plot './data/hermite.csv' using 1:2 with line lt -1 lw 2 title 'f(x)', '-' with points pt 6 ps 2 lc rgb 'red' title 'fixed points'
1,2
3,3
4,2
e